{
  "expo": {
    "name": "Вордл",
    "slug": "wordle-ios",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.extensi.one.wordle-ios"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
